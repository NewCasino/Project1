.Authenticated div.Col4-Content.noaccount{display: none;}
.Anonymous div.Col4-Content.needhelp{display: none;}
.SItem {border-radius: 0.5em 0.5em 0.5em 0.5em;height: 270px;}
.SItem > p {color: #FFFFFF;display: block;font-family: Myriad Pro;font-size: 13px;line-height: 20px;min-height: 60px;padding-left: 5%;}
.SliderTitle {display: block;font-family: H,HRoman,Arial,tahoma,sans-serif;font-size: 5em;font-weight: bold;line-height: 1em;margin-bottom: 0.233em;padding-left: 5%;padding-top: 30px;position: relative;text-shadow: 0 3px 3px #000000;}
.SliderTitle > a {text-decoration: none;}
.ActionZone {padding-left: 5%;padding-top: 10px;}
.pokerbutton {text-decoration: none;}
.Poker-Bonuses .Col4-Content {box-shadow: none;}
.row-fluid:nth-child(2) {  padding-top: 20px;}

.row-fluid .Col4 h3 {border-radius: 10px 10px 10px 10px;box-shadow: 0 1px 2px #000000;color: #333333;display: block;font-family: "Myriad Pro",Arial,Helvetica,sans-serif;font-size: 18px;font-weight: 700;height: 36px;line-height: 36px;padding-left: 14px;text-shadow: 0 1px 0 #FFFFFF;background: rgb(226,226,226);background: url(data:image/svg+xml;base64,);background: -moz-linear-gradient(top,  rgba(226,226,226,1) 0%, rgba(216,216,216,1) 26%, rgba(186,186,186,1) 49%, rgba(137,137,137,1) 74%, rgba(115,115,115,1) 96%, rgba(124,124,124,1) 100%);background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(226,226,226,1)), color-stop(26%,rgba(216,216,216,1)), color-stop(49%,rgba(186,186,186,1)), color-stop(74%,rgba(137,137,137,1)), color-stop(96%,rgba(115,115,115,1)), color-stop(100%,rgba(124,124,124,1)));background: -webkit-linear-gradient(top,  rgba(226,226,226,1) 0%,rgba(216,216,216,1) 26%,rgba(186,186,186,1) 49%,rgba(137,137,137,1) 74%,rgba(115,115,115,1) 96%,rgba(124,124,124,1) 100%);background: -o-linear-gradient(top,  rgba(226,226,226,1) 0%,rgba(216,216,216,1) 26%,rgba(186,186,186,1) 49%,rgba(137,137,137,1) 74%,rgba(115,115,115,1) 96%,rgba(124,124,124,1) 100%);background: -ms-linear-gradient(top,  rgba(226,226,226,1) 0%,rgba(216,216,216,1) 26%,rgba(186,186,186,1) 49%,rgba(137,137,137,1) 74%,rgba(115,115,115,1) 96%,rgba(124,124,124,1) 100%);background: linear-gradient(to bottom,  rgba(226,226,226,1) 0%,rgba(216,216,216,1) 26%,rgba(186,186,186,1) 49%,rgba(137,137,137,1) 74%,rgba(115,115,115,1) 96%,rgba(124,124,124,1) 100%);filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e2e2e2', endColorstr='#7c7c7c',GradientType=0 );}
.linkimage {  background-color: #000000;  background-position: 50% 100%;  background-size: 100% auto;  border-radius: 0.5em 0.5em 0.5em 0.5em;  display: block;  margin-bottom: 0;  margin-top: -8px;  max-width: 100%;  min-height: 22.5em;  opacity: 0.7;  position: relative;  text-decoration: none;  transition: opacity 0.3s linear 0s;  width: auto;  z-index: 0;}
.ie7 .linkimage,.ie8 .linkimage,.ie9 .linkimage { margin-top:0px;}
.linkimage.bonus1 {background-image: url("//cdn.everymatrix.com/ArtemisBetV3/_files/Background/artemispokerslide%201-to%20left%20side.jpg");}
.linkimage.bonus2 {background-image: url("//cdn.everymatrix.com/ArtemisBetV3/_files/Background/artemispokerslide2-between.jpg");}
.linkimage.bonus3 {background-image: url("//cdn.everymatrix.com/ArtemisBetV3/_files/Background/artemispokerslide3-To%20right%20side.jpg"); }
.row-fluid .Col4 h3 {  display: block;  position: relative;  z-index: 4;}
.row-fluid .Col4 h3.invisible {visibility: visible;}
.linkimage:hover{opacity: 1;-moz-opacity: 1;}
.linkimage.needhelp {background-image: url("//cdn.everymatrix.com/ArtemisBetV3/img/link-needhelp.jpg");background-repeat: no-repeat;background-size: 150% 100%;background-color:#fff;}
.linkimage.noaccount {background-image: url("//cdn.everymatrix.com/ArtemisBetV3/img/link-noaccount.jpg");background-repeat: no-repeat;background-size: auto auto;}
.linkimage .GoldButton {margin-left: 20px;margin-top: 100px;}
.NeedHelpText {color: #000000;display: block;font-family: Myriad Pro;font-size: 26px;font-weight: 700;padding-left: 10px;padding-top: 170px;text-align: center;text-shadow: 1px 0 0 #FFFFFF;}
.NeedHelpHours {clear: both;display: block;font-size: 20px;margin-top: 5px;}
@media only screen and (max-width: 1280px){.row-fluid .Col4.last { width : 48.5%; margin : 0 1.5% 1.5em 0; }}/* <= 1280 */