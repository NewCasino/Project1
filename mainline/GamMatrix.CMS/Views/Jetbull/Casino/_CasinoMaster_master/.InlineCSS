.GTprovider{ text-indent:0 !important; }
.GTprovider .GTicon{text-indent:0; }
.GFIcon{ display: block; height: 2.1666em; width: 3em; text-indent:0; }

.GFIcon img{ margin: 0.35em 0.75em 0; filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale"); /* Firefox 3.5+ */
    filter: gray; /* IE6-9 */ -webkit-filter: grayscale(100%); }
.GFActive .GFIcon img{ filter: none; -webkit-filter: none; }