	box-shadow: 1px 2px 2px 2px rgba(0, 0, 0, 0.7);
}
.LP-IM-link:hover{
	opacity: 1;
	-moz-opacity: 1;
	background-color#FFF;
	filter:alpha(opacity=100);
}
.LP-IM-link.link1{	background-image: url('//cdn.everymatrix.com/Jetbull/_files/landing/link1.jpg'); }
.LP-IM-link.link2{	background-image: url("//cdn.everymatrix.com/Jetbull/_files/landing/landing_casino.jpg") }
.LP-IM-link.link3{	background-image: url('//cdn.everymatrix.com/Jetbull/_files/landing/link3.jpg'); }
