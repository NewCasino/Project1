/******************/
/* GENERIC STYLES */
/******************/

/* fonts */
body, select, input, textarea, button { font-family : "Myriad", "Myriad Pro",Arial, Helvetica, "Nimbus Sans L", "Liberation Sans", FreeSans, clean, sans-serif; }
h1, h2, h3 { font-family : "Myriad", "Myriad Pro", Arial, Helvetica, "Nimbus Sans L", "Liberation Sans", FreeSans, clean, sans-serif; }

/* anchor style */
a { color : #f02222; text-decoration : none; }
a:visited { color : #900d0d; }
a:hover { color: #fff; }
a:active { color : #eee; }
a:focus { outline: thin dotted; }

/* Improve readability when focused and hovered in all browsers: h5bp.com/h */
a:hover, a:active { outline: 0; }

h1 { font-size : 3em; line-height : 1.5em; }

img { display : block; max-width : 100%; height : auto; border : none; }

/************/
/* CLEARFIX */
/************/

.Main { display : table-cell; *display : block; width : auto; }

.Body, .Container, .Box, .Main { *zoom : 1; }

.Body:after, .Container:after, .Box:after, .Main:after,.row-fluid:after { clear : both; display : block; visibility : hidden; overflow : hidden; height : 0 !important; line-height : 0; font-size : xx-large; content : " x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x x "; }

/************/
/* BOX FLOW */
/************/

.SideBar { float : left; width : 25%; }
.AuxBar { float : right; width : 16.6666%; }
.Main {  }

.Box { margin : 1.5em 0.75em; }
.Main .Box { margin : 1.5em 0; }

/***********/
/* BUTTONS */
/***********/
.LP .Button{ display : block; padding : 0.5em 1em; background : #B40808 url("//cdn.everymatrix.com/Jetbull/_files/landing/gradient.png") center bottom no-repeat; background-size : 100% 200%; color : #fff; font-size : 1.5em; line-height : 1em; font-weight : bold; text-align : center; text-decoration : none; border-radius : 0.5em; box-shadow : 0 0.25em 0.25em #000; text-shadow : 0 -1px 0 #600;   transition: all 0.125s ease-in-out;  -moz-transition: all 0.125s ease-in-out; /* Firefox 4 */   -webkit-transition: all 0.125s ease-in-out; /* Safari and Chrome */    -o-transition: all 0.125s ease-in-out; /* Opera */}
.LP .Button:hover { background-color : #f00; color : #fff; box-shadow : 0 0.3333em 0.3333em #000; text-shadow : 0 -1px 0 #900; }
.LP .Button:active { background-color : #666; background-position : center top; color : #fff; box-shadow : inset 0 0.25em 0.25em #000; text-shadow : 0 0.125em 0.125em #000; }
.LP .BigButton { background: url("//cdn.everymatrix.com/Jetbull/_files/landing/bg-button.png") repeat-x scroll left -78px #4E4E4E;  border-bottom: 0.0555em solid #000000;  color: #FFFFFF; display: block; }

/****************/
/* HIDING STUFF */
/****************/
.ir { display : block; text-indent : -999em; overflow : hidden; background-repeat : no-repeat; text-align : left; direction : ltr; }
.ir br { display : none; }
.hidden, .Hidden { display : none !important; visibility : hidden; }
.visuallyhidden { border : 0; clip : rect(0 0 0 0); height : 1px; margin : -1px; overflow : hidden; padding : 0; position : absolute; width : 1px; }
.visuallyhidden.focusable:active, .visuallyhidden.focusable:focus { clip : auto; height : auto; margin : 0; overflow : visible; position : static; width : auto; }
.invisible { visibility : hidden; }

/*************************/
/* VISUAL DEBUGGING MODE */
/*************************/
#viewgrid{background:none repeat scroll 0 0 rgba(0,0,0,0.4);color:#FFFFFF;left:0.1%;padding:3px 12px;position:fixed;text-shadow:none;top:5px;width:auto;z-index:999;}
.DebugColumns { display : none; position : absolute; top : 0; left : 0; width : 100%; height : 100%; background : transparent url("//cdn.everymatrix.com/Jetbull/_files/landing/baseline-18.png") left top repeat; z-index : -1; }
.Debug .DebugColumns { display : block; }
.Debug18 { background-image : url(); }
.Debug14 { background-image : url(); }
.Debug8 { background-image : url(); }
.Debug3 { background-image : url(); }
	.DebugCol { display : block; float : left; height : 100%; }
	.Debug18 .DebugCol { width : 5.5555%; }
	.Debug14 .DebugCol { width : 7.1428%; }
	.Debug8 .DebugCol { width : 12.5%; }
	.Debug3 .DebugCol { width : 33.3333%; }
	.Debug3 .DX { width : 0.3125%; margin : 0 -0.3125%; background : black; }
		.DebugContent { display : block; height : 100%; margin : 0 6.18%; background : transparent url("//cdn.everymatrix.com/Jetbull/_files/landing/bgw-30.png") left top repeat; }
		.Debug8 .DebugContent { margin : 0 5%; }
		.Debug3 .DebugContent { margin : 0 10.3125%; }
		.Debug3 .D3-1 .DebugContent { margin-left : 4.6875%; }
		.Debug3 .D3-2 .DebugContent {  }
		.Debug3 .D3-3 .DebugContent { margin-right : 4.6875%; }
		.Debug18 .DebugCol:nth-child(1) .DebugContent, .Debug18 .DebugCol:nth-child(2) .DebugContent, .Debug18 .DebugCol:nth-child(3) .DebugContent, .Debug18 .DebugCol:nth-child(16) .DebugContent, .Debug18 .DebugCol:nth-child(17) .DebugContent, .Debug18 .DebugCol:nth-child(18) .DebugContent, .Debug14 .DebugCol:nth-child(1) .DebugContent, .Debug14 .DebugCol:nth-child(14) .DebugContent, .Debug8 .DebugCol:nth-child(1) .DebugContent, .Debug8 .DebugCol:nth-child(8) .DebugContent { background-image : url("//cdn.everymatrix.com/Jetbull/_files/landing/bgw-10.png"); }
.Debug #framework, .DebugRow #framework { background : transparent url("//cdn.everymatrix.com/Jetbull/_files/landing/baseline-18.png") left top repeat; }
#viewgrid, .DebugColumns, .Debug .DebugColumns { display : none; }

