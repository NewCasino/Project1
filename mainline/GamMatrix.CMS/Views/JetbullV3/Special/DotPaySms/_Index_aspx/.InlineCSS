html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {    margin: 0;    padding: 0;    border: 0;    outline: 0;    font-size: 100%;    vertical-align: baseline;    background: transparent;}
body { line-height: 1; }
blockquote, q { quotes: none;}
ins { text-decoration: none; }
del { text-decoration: line-through; }
table { border-collapse: collapse; border-spacing: 0; }
legend { display: none; }
ul li { list-style-type: none; }
input:focus { outline: none }
a { text-decoration: none;}
input { margin: 0 }
body {    padding-bottom: 10px;    font: small/1.2 "Trebuchet MS", Arial, sans-serif;    color: #222;    background: #fff url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/body-bg.jpg) 0 0 repeat-x;}
#wrap {    width: 658px;    margin: 0 auto;    background: transparent url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/top.jpg) 0 0 no-repeat;}
h1 { margin-bottom: 15px; }
h1, h1 span {    position: relative;    height: 360px;    line-height: 451px;}
h1 span {    position: absolute;    top: 0;    left: 0;    display: block;    width: 100%;    background: transparent url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/top.jpg) 0 0 no-repeat;}
.box, .box2 {    width: 462px;    margin: 0 0 10px -18px;}
.box h2, .box h2 span,.box2 h2, .box2 h2 span {    position: relative;    width: 462px;    height: 58px;}
.box h2 span, .box2 h2 span {    position: absolute;    top: 0;    left: 0;}
.box h2 span {    background: transparent url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/headers-3.jpg) 0 0 no-repeat;}
.box2 h2 span{    background: transparent url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/headers2-3.jpg) 0 0 no-repeat;}
#zeton6 h2 span { background-position: 0 -59px; }
#zeton9 h2 span { background-position: 0 -118px; }
#zeton19 h2 span { background-position: 0 -177px; }
#zeton25 h2 span { background-position: 0 -235px; }
#pakiet25 h2 span { background-position: 0 -59px; }
#pakiet50 h2 span { background-position: 0 -118px; }
#pakiet100 h2 span { background-position: 0 -177px; }
.box-content {    min-height: 79px;    padding: 3px 0 0 50px;    font-size: 120%;    color: #fff;}
p.firstp {    padding-top: 15px;}
#zeton5 .box-content, #pakiet10 .box-content { background: #7ebc7e url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/box1-2.jpg) left bottom no-repeat; }
#zeton6 .box-content, #pakiet25 .box-content { background: #f2a547 url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/box2-2.jpg) left bottom no-repeat; }
#zeton9 .box-content, #pakiet50 .box-content { background: #F24786 url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/box3-2.jpg) left bottom no-repeat; }
#zeton19 .box-content, #pakiet100 .box-content { background: #c247f2 url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/box4-2.jpg) left bottom no-repeat; }
#zeton25 .box-content { background: #7247f2 url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/box5-2.jpg) left bottom no-repeat; }
.box-content p span{display:block;font-size:80%;padding:10px 0}
.box-content a { color: #dde; }
.box-content a:hover { color: #eef; text-decoration: underline; }
#footer {    width: 623px;    height: 37px;    margin-top: 1.5em;    padding-left: 15px;    line-height: 37px;    color: #fff;    font-size: 115%;	text-align: center;    text-transform: uppercase;    background: #28B1CF url(//cdn.everymatrix.com/Shared/_files/Special/DotpaySMS/footer.jpg) 0 0 no-repeat;}
#footer a {	    color: #fff;}
