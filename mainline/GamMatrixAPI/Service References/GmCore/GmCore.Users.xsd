      <xs:enumeration value="SelfExclusionFor1Year" />
      <xs:enumeration value="SelfExclusionPermanent" />
      <xs:enumeration value="SelfExclusionUntilSelectedDate" />
      <xs:enumeration value="SelfExclusionFor5Years" />
      <xs:enumeration value="SelfExclusionFor7Years" />
      <xs:enumeration value="CoolOffFor24Hours" />
      <xs:enumeration value="CoolOffFor7Days" />
      <xs:enumeration value="CoolOffFor30Days" />
      <xs:enumeration value="CoolOffFor3Months" />
      <xs:enumeration value="CoolOffUntilSelectedDate" />
      <xs:enumeration value="CoolOffNone" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="SelfExclusionPeriod" nillable="true" type="tns:SelfExclusionPeriod" />
</xs:schema>